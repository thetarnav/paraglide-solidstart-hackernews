{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"noEmit": true,
		"strict": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"allowJs": true,
		"types": ["vinxi/client"],
		"jsxImportSource": "solid-js",
		"jsx": "preserve",
		"paths": {
			"~/*": ["./src/*"]
		}
	},
	"include": ["src"]
}
