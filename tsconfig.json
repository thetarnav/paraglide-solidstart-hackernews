{
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "types": ["solid-start/env"],
        "jsxImportSource": "solid-js",
        "jsx": "preserve",
        "strict": true,
        "paths": {
            "~/*": ["./src/*"]
        }
    }
}
